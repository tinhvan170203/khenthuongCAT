                            || allRole.indexOf('xem-khen-thuong-doi') !== -1 || allRole.indexOf('xem-danh-sach-can-bo') !== -1
                            || allRole.indexOf('xem-thi-dua-thang') !== -1 || allRole.indexOf('xem-thi-dua-nam') !== -1
                            || allRole.indexOf('xem-xep-loai-dang-vien') !== -1 || allRole.indexOf('xem-ki-luat-ca-nhan') !== -1
                            || allRole.indexOf('xem-khen-thuong-ca-nhan') !== -1
                             ){ %>
                                                <li class="menu-admin-item">
                                                    <a href="#" data-bs-toggle="modal" data-bs-target="#modalRedirect" data-bs-backdrop='static'>
                                                        <i class="fas fa-house-user menu-item-icon"></i>
                                                        <h4 class="menu-item-title">Quản trị chi tiết đơn vị</h4>
                                                    </a>
                                                </li>
                                                <%  } %>
                                                    <% if(allRole.indexOf('xem-danh-sach-van-ban') !== -1){%>
                                                        <li class="menu-admin-item">
                                                            <a href="/loaivanban">
                                                                <i class="fas fa-scroll menu-item-icon"></i>
                                                                <h4 class="menu-item-title">Quản trị loại văn bản</h4>
                                                            </a>
                                                        </li>
                                                        <%   } %>
                                                            <% if(allRole.indexOf('xem-danh-sach-van-ban') !== -1){%>
                                                                <li class="menu-admin-item">
                                                                    <a href="/quantrivanban">
                                                                        <i class="fas fa-scroll menu-item-icon"></i>
                                                                        <h4 class="menu-item-title">Quản trị văn bản</h4>
                                                                    </a>
                                                                </li>
                                                                <%   } %>
                                                                    <% if(allRole.indexOf('xem-danh-sach-van-ban') !== -1){%>
                                                                        <li class="menu-admin-item active">
                                                                            <a href="#">
                                                                                <i class="fas fa-scroll menu-item-icon"></i>
                                                                                <h4 class="menu-item-title">Quản trị CQBH văn bản</h4>
                                                                            </a>
                                                                        </li>
                                                                        <%   } %>
                                                                            <% if(allRole.indexOf('xem-hoat-dong-tieu-bieu') !== -1){%>
                                                                                <li class="menu-admin-item">
                                                                                    <a href="/vannghethethao/quantri/hoatdongtieubieu">
                                                                                        <i class="fas fa-running menu-item-icon"></i>
                                                                                        <h4 class="menu-item-title">Hoạt động VN - TT tiêu biểu</h4>
                                                                                    </a>
                                                                                </li>
                                                                                <%   } %>
                                                                                    <% if(allRole.indexOf('xem-mo-hinh') !== -1){%>
                                                                                        <li class="menu-admin-item">
                                                                                            <a href="/vannghethethao/quantri/mohinhtieubieu">
                                                                                                <i class="fab fa-studiovinari menu-item-icon"></i>
                                                                                                <h4 class="menu-item-title">Mô hình VHVN - TT nổi bật</h4>
                                                                                            </a>
                                                                                        </li>
                                                                                        <%   } %>
                                                                                            <% if(allRole.indexOf('xem-ket-qua-the-thao') !== -1
                                                                            || allRole.indexOf('xem-mon-the-thao') !== -1
                                                                             || allRole.indexOf('xem-ca-nhan-the-thao-dien-hinh') !== -1
                                                                            ){%>
                                                                                                <li class="menu-admin-item">
                                                                                                    <a href="/vannghethethao/quantri/thethao">
                                                                                                        <i class="fas fa-biking menu-item-icon"></i>
                                                                                                        <h4 class="menu-item-title">Quản trị thể thao</h4>
                                                                                                    </a>
                                                                                                </li>
                                                                                                <%   } %>
                                                                                                    <% if(allRole.indexOf('xem-ket-qua-van-nghe') !== -1
                                                                            || allRole.indexOf('xem-the-loai-van-nghe') !== -1
                                                                             || allRole.indexOf('xem-ca-nhan-van-nghe-dien-hinh') !== -1
                                                                            ){%>
                                                                                                        <li class="menu-admin-item">
                                                                                                            <a href="/vannghethethao/quantri/vannghe">
                                                                                                                <i class="fas fa-music menu-item-icon"></i>
                                                                                                                <h4 class="menu-item-title">Quản trị văn hóa văn nghệ</h4>
                                                                                                            </a>
                                                                                                        </li>
                                                                                                        <%   } %>
                                                                                                            <% if(allRole.indexOf('xem-chuyen-muc') !== -1){%>
                                                                                                                <li class="menu-admin-item">
                                                                                                                    <a href="/tuyentruyen/quantri/chuyenmuc">
                                                                                                                        <i class="fab fa-apple menu-item-icon"></i>
                                                                                                                        <h4 class="menu-item-title">Quản trị chuyên mục</h4>
                                                                                                                    </a>
                                                                                                                </li>
                                                                                                                <%   } %>
                                                                                                                    <% if(allRole.indexOf('xem-muc-tin') !== -1){%>
                                                                                                                        <li class="menu-admin-item">
                                                                                                                            <a href="#">
                                                                                                                                <i class="fab fa-edge menu-item-icon"></i>
                                                                                                                                <h4 class="menu-item-title">Quản trị mục tin</h4>
                                                                                                                            </a>
                                                                                                                        </li>
                                                                                                                        <%   } %>
                                                                                                                            <% if(allRole.indexOf('xem-tin-bai') !== -1){%>
                                                                                                                                <li class="menu-admin-item">
                                                                                                                                    <a href="/tuyentruyen/quantri/tinbai">
                                                                                                                                        <i class="fas fa-fan menu-item-icon"></i>
                                                                                                                                        <h4 class="menu-item-title">Quản trị tin bài</h4>
                                                                                                                                    </a>
                                                                                                                                </li>
                                                                                                                                <%   } %>
                                                                                                                                    <% if(allRole.indexOf('xem-cuoc-thi-tuyen-truyen') !== -1){%>
                                                                                                                                        <li class="menu-admin-item">
                                                                                                                                            <a href="/tuyentruyen/quantri/ketquathi">
                                                                                                                                                <i class="fab fa-modx menu-item-icon"></i>
                                                                                                                                                <h4 class="menu-item-title">Quản trị thi tuyên truyền</h4>
                                                                                                                                            </a>
                                                                                                                                        </li>
                                                                                                                                        <%   } %>
                                                                                                                                            <% if(allRole.indexOf('xem-ket-qua-kiem-tra-px03') !== -1){%>
                                                                                                                                                <li class="menu-admin-item">
                                                                                                                                                    <a href="/congtacdieulenh/quantri">
                                                                                                                                                        <i class="fab fa-shirtsinbulk menu-item-icon"></i>
                                                                                                                                                        <h4 class="menu-item-title">Kiểm tra điều lệnh CA tỉnh</h4>
                                                                                                                                                    </a>
                                                                                                                                                </li>
                                                                                                                                                <%   } %>
                                                                                                                                                    <% if(allRole.indexOf('xem-tinh-huong-gia-dinh') !== -1){%>
                                                                                                                                                        <li class="menu-admin-item">
                                                                                                                                                            <a href="/congtacdieulenh/quantri/tinhhuong">
                                                                                                                                                                <i class="fas fa-skull-crossbones menu-item-icon"></i>
                                                                                                                                                                <h4 class="menu-item-title">Quản trị tình huống giả định</h4>
                                                                                                                                                            </a>
                                                                                                                                                        </li>
                                                                                                                                                        <%   } %>
                                                                                                                                                            <% if(allRole.indexOf('xem-kiem-tra-tinh-huong-gia-dinh') !== -1){%>
                                                                                                                                                                <li class="menu-admin-item">
                                                                                                                                                                    <a href="/congtacdieulenh/quantri/kiemtra/tinhhuong">
                                                                                                                                                                        <i class="fas fa-user-secret menu-item-icon"></i>
                                                                                                                                                                        <h4 class="menu-item-title">Kiểm tra tình huống giả định</h4>
                                                                                                                                                                    </a>
                                                                                                                                                                </li>
                                                                                                                                                                <%   } %>
                                                                                                                                                                    <% if(allRole.indexOf('xem-kiem-tra-quan-su-vo-thuat') !== -1){%>
                                                                                                                                                                        <li class="menu-admin-item">
                                                                                                                                                                            <a href="/congtacdieulenh/quantri/kiemtra/dieulenhquansu">
                                                                                                                                                                                <i class="fas fa-fan menu-item-icon"></i>
                                                                                                                                                                                <h4 class="menu-item-title">Kiểm tra điều lệnh, võ thuật</h4>
                                                                                                                                                                            </a>
                                                                                                                                                                        </li>
                                                                                                                                                                        <%   } %>
                                                                                                                                                                            <% if(allRole.indexOf('xem-kiem-tra-the-luc') !== -1){%>
                                                                                                                                                                                <li class="menu-admin-item">
                                                                                                                                                                                    <a href="/congtacdieulenh/quantri/kiemtra/theluc">
                                                                                                                                                                                        <i class="fas fa-swimmer menu-item-icon"></i>
                                                                                                                                                                                        <h4 class="menu-item-title">Kiểm tra thể lực</h4>
                                                                                                                                                                                    </a>
                                                                                                                                                                                </li>
                                                                                                                                                                                <%   } %>
                                                                                                                                                                                    <% if(allRole.indexOf('xem-kiem-tra-ban-sung') !== -1){%>
                                                                                                                                                                                        <li class="menu-admin-item">
                                                                                                                                                                                            <a href="/congtacdieulenh/quantri/kiemtra/bansung">
                                                                                                                                                                                                <i class="fas fa-map-signs menu-item-icon"></i>
                                                                                                                                                                                                <h4 class="menu-item-title">Kiểm tra bắn đạn thật</h4>
                                                                                                                                                                                            </a>
                                                                                                                                                                                        </li>
                                                                                                                                                                                        <%   } %>
                </ul>
            </div>
            <div class="col-xl-10" id="chuyenmuc">
                <div id="tieudetrang" class="animate__animated animate__fadeInUp">
                    <h3 class="tieudetrang">Quản trị cơ quan ban hành văn bản</h3>
                </div>
                <%if(allRole.indexOf('them-van-ban') !== -1){%>
                    <button style="display: block;float: right; margin: 12px 0;font-size: 12px;" class="btn btn-success" data-bs-toggle="modal" data-bs-target="#modalAddCuocthi" data-bs-backdrop="static"><i class="fas fa-user-shield" style="margin-right: 5px"></i>Thêm mới</button>
                    <% } %>
                        <div class="animate__animated animate__fadeIn" id="cuocthi">
                            <div class="tinai-wrapper-body">
                                <table class="table table-striped table-bordered" id="ketquaTable" style="width: 100%">
                                    <thead>
                                        <th>STT</th>
                                        <th>Tên Cơ quan ban hành văn bản</th>
                                        <th></th>
                                        <th></th>
                                    </thead>
                                    <tbody></tbody>
                                </table>
                            </div>
                        </div>
            </div>
        </div>


        <!-- Toast  Thêm VB thành công-->
        <div class="position-fixed bottom-0 end-0 p-3" style="z-index: 1111">
            <div id="add-toast" class="toast hide" role="alert" aria-live="assertive" aria-atomic="true">
                <div class="toast-header text-white bg-primary">
                    <strong class="me-auto">Thành công</strong>
                    <small>Vừa xong</small>
                    <button type="button" class="btn-close" data-bs-dismiss="toast" aria-label="Close"></button>
                </div>
                <div class="toast-body">
                    Thêm mới thành công.
                </div>
            </div>
        </div>
        <!-- End Toast  -->
        <!-- Toast  Edit VB thành công-->
        <div class="position-fixed bottom-0 end-0 p-3" style="z-index: 1111">
            <div id="edit-toast" class="toast hide" role="alert" aria-live="assertive" aria-atomic="true">
                <div class="toast-header text-white bg-primary">
                    <strong class="me-auto">Thành công</strong>
                    <small>Vừa xong</small>
                    <button type="button" class="btn-close" data-bs-dismiss="toast" aria-label="Close"></button>
                </div>
                <div class="toast-body">
                    Cập nhật thành công.
                </div>
            </div>
        </div>
        <!-- End Toast  -->
        <!-- Toast  Edit VB thành công-->
        <div class="position-fixed bottom-0 end-0 p-3" style="z-index: 1111">
            <div id="delete-toast" class="toast hide" role="alert" aria-live="assertive" aria-atomic="true">
                <div class="toast-header text-white bg-primary">
                    <!-- <img src="" class="rounded me-2" alt="..."> -->
                    <strong class="me-auto">Thành công</strong>
                    <small>Vừa xong</small>
                    <button type="button" class="btn-close" data-bs-dismiss="toast" aria-label="Close"></button>
                </div>
                <div class="toast-body">
                    Xóa thành công.
                </div>
            </div>
        </div>
        <!-- End Toast  -->
        <span id='variableJSON' hidden>
            <%= JSON.stringify(allRole); %>
        </span>
    </div>
    <script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.2/jquery.min.js">
    </script>
    <script src="https://code.jquery.com/jquery-3.6.0.js"></script>
    <script src="//cdn.datatables.net/1.10.25/js/jquery.dataTables.min.js"></script>
    <script type="text/javascript" language="javascript" src="https://cdn.datatables.net/select/1.2.1/js/dataTables.select.min.js"></script>
    <script src="/javascripts/vanbanquydinh/coquanbanhanh.js"></script>
    <script src="/javascripts/logout.js"></script>

</body>
