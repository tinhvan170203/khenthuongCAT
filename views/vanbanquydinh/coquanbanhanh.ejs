<!DOCTYPE html>
<html lang="en">

<head>
    <title>
        Quản trị cơ quan ban hành văn bản
    </title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM" crossorigin="anonymous"></script>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.2/css/all.min.css" />
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <link rel='stylesheet' href='//cdn.datatables.net/1.10.25/css/jquery.dataTables.min.css' />
    <!-- <link rel='stylesheet' href='/stylesheets/components/navbar/style.css' /> -->
    <link rel='stylesheet' href='/stylesheets/congtactuyentruyen/style.css' />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/animate.css/4.1.1/animate.min.css" />

</head>

<body>

    <div class="container-fluid">
        <div class="row">
            <%-include('../components/nav')%>
        </div>
        <div class="row">
            <div class="modal fade" id="modalRedirect" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
                <div class="modal-dialog">
                    <div class="modal-content" id="modal-content">
                        <div class="modal-header">
                            <h5 class="modal-title" id="exampleModalLabel">Quản trị chi tiết đơn vị cấp phòng, huyện, thị xã, thành phố</h5>
                            <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                        </div>
                        <div class="modal-body">
                            <div class="container-fluid">
                                <form id="redirectForm" class="row g-3">
                                    <div class="col-xl-12">
                                        <label for="ten" class="form-label">Chọn đơn vị muốn quản trị chi tiết tập thể đơn vị hoặc cá nhân: </label>
                                        <select id="donvi" class="form-select">
                                             <option value="" hidden disabled selected>-- Chọn mục này --</option>
                                             <% if(list.length > 0){
                                                  list.forEach(i=>{ %>
                                                       <option value="<%=i._id%>"><%=i.ten%></option>
                                               <%   })
                                             } %>
                                        </select>
                                    </div>
                                    <div class="col-12">
                                        <button id="redirect" class="btn btn-primary add" disabled type="submit">Đi tới trang</button>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!-- Modal add cuộc thi -->
            <div class="modal fade" id="modalAddCuocthi" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
                <div class="modal-dialog">
                    <div class="modal-content" id="modal-content">
                        <div class="modal-header">
                            <h5 class="modal-title" id="exampleModalLabel">Thêm mới cơ quan ban hành văn bản</h5>
                            <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                        </div>
                        <div class="modal-body">
                            <div class="container-fluid">
                                <form id="cuocthiForm" class="row g-3">
                                    <div class="col-xl-12">
                                        <label for="ten" class="form-label">Tên cơ quan ban hành văn bản: </label>
                                        <input type="text" name="ten" class="form-control" id="ten" required>
                                    </div>
                                    <div class="col-12">
                                        <button class="btn btn-primary add" type="submit">Thêm mới</button>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!-- End modal add cuộc thi  -->
            <!-- Modal edit cuộc thi -->
            <div class="modal fade" id="modalEditCuocthi" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
                <div class="modal-dialog">
                    <div class="modal-content" id="modal-content">
                        <div class="modal-header">
                            <h5 class="modal-title" id="exampleModalLabel">Chỉnh sửa cơ quan ban hành văn bản</h5>
                            <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                        </div>
                        <div class="modal-body">
                            <div class="container-fluid">
                                <form id="EditCuocthiForm" class="row g-3">
                                    <div class="col-xl-12">
                                        <label for="tenEdit" class="form-label">Tên cơ quan ban hành văn bản: </label>
                                        <input type="text" name="tenEdit" class="form-control" id="tenEdit" required>
                                    </div>
                                    <div class="col-12">
                                        <button class="btn btn-primary edit" type="submit">Cập nhật</button>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!-- End modal edit cuộc thi  -->
            <!-- menu-left  -->
            <div class="col-xl-2" id="menu-chuyenmuc">
                <ul class="menu-admin">
                    <% if(allRole.indexOf('xem-danh-sach-phan-quyen') !== -1){ %>
                        <li class="menu-admin-item">
                            <a href="/quantrihethong/nhomquyen">
                                <i class="fas fa-project-diagram menu-item-icon"></i>
                                <h4 class="menu-item-title">Quản trị nhóm quyền</h4>
                            </a>
                        </li>
                        <%  } %>
                            <% if(allRole.indexOf('xem-danh-sach-nguoi-dung') !== -1){ %>
                                <li class="menu-admin-item">
                                    <a href="/quantrihethong/quantritaikhoan">
                                        <i class="fas fa-user-shield menu-item-icon"></i>
                                        <h4 class="menu-item-title">Quản trị tài khoản</h4>
                                    </a>
                                </li>
                                <%  } %>
