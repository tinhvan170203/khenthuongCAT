            <!-- Modal CRUD văn bản  -->
            <div class="modal fade" id="modalCRUDvanban" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
                <div class="modal-dialog">
                    <div class="modal-content" id="modal-content">
                        <div class="modal-header">
                            <h5 class="modal-title" id="exampleModalLabel">Văn bản quy định, hướng dẫn, chỉ đạo liên quan đến các mặt công tác chính trị trong Công an tỉnh</h5>
                            <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                        </div>
                        <div class="modal-body">
                            <div class="container-fluid">
                                <form id="vanbanForm" class="row g-3" enctype="multipart/form-data" action="/addVB" method="POST">
                                    <div class="col-md-4">
                                        <label for="soVB" class="form-label">Số văn bản : </label>
                                        <input type="text" name="soVB" class="form-control" id="soVB" required>
                                    </div>
                                    <div class="col-md-8">
                                        <label for="tenVB" class="form-label">Tên văn bản :</label>
                                        <input type="text" name="tenVB" class="form-control" id="tenVB" required>
                                    </div>
                                    <div class="col-md-4">
                                        <label for="kyhieuVB" class="form-label">Ký hiệu văn bản :</label>
                                        <div class="input-group has-validation">
                                            <input type="text" name="kyhieuVB" class="form-control" id="kyhieuVB" aria-describedby="inputGroupPrepend" required>
                                        </div>
                                    </div>
                                    <div class="col-md-8">
                                        <label for="trichyeu" class="form-label">Trích yếu nội dung văn bản :</label>
                                        <input type="text" name="trichyeu" class="form-control" id="trichyeu" required>
                                    </div>
                                    <div class="col-md-3">
                                        <label for="loaiVB" class="form-label">Loại văn bản :</label>
                                        <select class="form-select" name="loaiVB" id="loaiVB" required>
                                            <option selected disabled value="">Chọn loại văn bản</option>
                                            <%loaiVB.forEach(item=>{ %>
                                                <option value="<%=item._id%>"><%=item.loaiVB%></option>
                                         <%   })%>
                                        </select>
                                    </div>
                                    <div class="col-md-3">
                                        <label for="coquanbanhanh" class="form-label">Cơ quan ban hành :</label>
                                        <select class="form-select" name="coquanbanhanh" id="coquanbanhanh" required>
                                            <option selected disabled value="">Chọn cơ quan ban hành</option>
                                            <%coquanbanhanh.forEach(item=>{%>
                                                <option value="<%=item._id%>"><%=item.coquanbanhanh%></option>
                                          <%  })%>
                                        </select>
                                    </div>
                                    <div class="col-md-3">
                                        <label for="ngaybanhanh" class="form-label">Ngày ban hành :</label>
                                        <input type="date" name="ngaybanhanh" class="form-control" id="ngaybanhanh" required>
                                    </div>
                                    <div class="col-md-3">
                                        <label for="nguoiky" class="form-label">Người kí :</label>
                                        <input type="text" name="nguoiky" class="form-control" id="nguoiky" required>
                                    </div>
                                    <div class="col-md-12">
                                        <label for="file" class="form-label">Tệp đính kèm :</label>
                                        <input type="file" name="file" class="form-control" id="file" accept=".pdf" required>
                                    </div>
                                    <div class="col-12">
                                        <button class="btn btn-primary add" type="submit">Thêm mới</button>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
