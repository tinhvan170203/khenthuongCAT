                                                                                                                            <a href="/tuyentruyen/quantri/ketquathi">
                                                                                                                                <i class="fab fa-modx menu-item-icon"></i>
                                                                                                                                <h4 class="menu-item-title">Quản trị thi tuyên truyền</h4>
                                                                                                                            </a>
                                                                                                                        </li>
                                                                                                                        <%   } %>
                                                                                                                            <% if(allRole.indexOf('xem-ket-qua-kiem-tra-px03') !== -1){%>
                                                                                                                                <li class="menu-admin-item">
                                                                                                                                    <a href="/congtacdieulenh/quantri">
                                                                                                                                        <i class="fab fa-shirtsinbulk menu-item-icon"></i>
                                                                                                                                        <h4 class="menu-item-title">Kiểm tra điều lệnh CA tỉnh</h4>
                                                                                                                                    </a>
                                                                                                                                </li>
                                                                                                                                <%   } %>
                                                                                                                                    <% if(allRole.indexOf('xem-tinh-huong-gia-dinh') !== -1){%>
                                                                                                                                        <li class="menu-admin-item">
                                                                                                                                            <a href="/congtacdieulenh/quantri/tinhhuong">
                                                                                                                                                <i class="fas fa-skull-crossbones menu-item-icon"></i>
                                                                                                                                                <h4 class="menu-item-title">Quản trị tình huống giả định</h4>
                                                                                                                                            </a>
                                                                                                                                        </li>
                                                                                                                                        <%   } %>
                                                                                                                                            <% if(allRole.indexOf('xem-kiem-tra-tinh-huong-gia-dinh') !== -1){%>
                                                                                                                                                <li class="menu-admin-item">
                                                                                                                                                    <a href="/congtacdieulenh/quantri/kiemtra/tinhhuong">
                                                                                                                                                        <i class="fas fa-user-secret menu-item-icon"></i>
                                                                                                                                                        <h4 class="menu-item-title">Kiểm tra tình huống giả định</h4>
                                                                                                                                                    </a>
                                                                                                                                                </li>
                                                                                                                                                <%   } %>
