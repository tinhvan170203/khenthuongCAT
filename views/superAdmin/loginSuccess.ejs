                                                                                <li class="menu-admin-item">
                                                                                    <a href="/vannghethethao/quantri/thethao">
                                                                                        <i class="fas fa-biking menu-item-icon"></i>
                                                                                        <h4 class="menu-item-title">Quản trị thể thao</h4>
                                                                                    </a>
                                                                                </li>
                                                                                <%   } %>
                                                                                    <% if(allRole.indexOf('xem-ket-qua-van-nghe') !== -1
                                                                            || allRole.indexOf('xem-the-loai-van-nghe') !== -1
                                                                             || allRole.indexOf('xem-ca-nhan-van-nghe-dien-hinh') !== -1
                                                                            ){%>
                                                                                        <li class="menu-admin-item">
                                                                                            <a href="/vannghethethao/quantri/vannghe">
                                                                                                <i class="fas fa-music menu-item-icon"></i>
                                                                                                <h4 class="menu-item-title">Quản trị văn hóa văn nghệ</h4>
                                                                                            </a>
                                                                                        </li>
                                                                                        <%   } %>
                                                                                            <% if(allRole.indexOf('xem-chuyen-muc') !== -1){%>
                                                                                                <li class="menu-admin-item">
                                                                                                    <a href="/tuyentruyen/quantri/chuyenmuc">
                                                                                                        <i class="fab fa-apple menu-item-icon"></i>
                                                                                                        <h4 class="menu-item-title">Quản trị chuyên mục</h4>
                                                                                                    </a>
                                                                                                </li>
                                                                                                <%   } %>
                                                                                                    <% if(allRole.indexOf('xem-muc-tin') !== -1){%>
                                                                                                        <li class="menu-admin-item">
                                                                                                            <a href="/tuyentruyen/quantri/muctin">
                                                                                                                <i class="fab fa-edge menu-item-icon"></i>
                                                                                                                <h4 class="menu-item-title">Quản trị mục tin</h4>
                                                                                                            </a>
                                                                                                        </li>
                                                                                                        <%   } %>
                                                                                                            <% if(allRole.indexOf('xem-tin-bai') !== -1){%>
                                                                                                                <li class="menu-admin-item">
                                                                                                                    <a href="/tuyentruyen/quantri/tinbai">
