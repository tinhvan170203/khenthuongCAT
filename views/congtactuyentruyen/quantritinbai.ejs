                                             <% if(list.length > 0){
                                                  list.forEach(i=>{ %>
                                                       <option value="<%=i._id%>"><%=i.ten%></option>
                                               <%   })
                                             } %>
                                        </select>
                                    </div>
                                    <div class="col-12">
                                        <button id="redirect" class="btn btn-primary add" disabled type="submit">Đi tới trang</button>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!-- Modal add tin bài -->
            <div class="modal fade" id="modalAddTinbai" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
