                        </div>
                        <div class="modal-body">
                            <div class="container-fluid">
                                <form id="tinbaiForm" class="row g-3">
                                    <div class="col-xl-6">
                                        <label for="tieude" class="form-label">Tiêu đề tin bài : </label>
                                        <input type="text" name="tieude" class="form-control" id="tieude" required>
                                    </div>
                                    <div class="col-xl-6">
                                        <label for="tacgia" class="form-label">Tác giả : </label>
                                        <input type="text" name="tacgia" class="form-control" id="tacgia" required>
                                    </div>
                                    <div class="col-xl-6">
                                        <label for="chuyenmucList" class="form-label">Chuyên mục : </label>
                                        <select class="form-select" name="chuyenmucList" id="chuyenmucList" required>
                                          <option selected disabled hidden >--- Chọn chuyên mục tin bài ---</option>
                                          <% if(chuyenmucList.length > 0) { 
                                                chuyenmucList.forEach(chuyenmuc=>{ %>
                                                <option value="<%=chuyenmuc._id%>"><%=chuyenmuc.tenchuyenmuc%></option>
                                                <%  })
                                            } %>
                                       </select>
                                    </div>
                                    <div class="col-xl-6">
                                        <label for="muctinList" class="form-label">Mục tin : </label>
                                        <select class="form-select" name="muctinList" id="muctinList" required>
                                          <option selected disabled hidden >--- Chọn mục tin của tin bài ---</option>
                                       </select>
                                    </div>
                                    <div class="col-xl-6">
                                        <label for="donviphoihop" class="form-label">Đơn vị phối hợp : </label>
                                        <select name="donviphoihop" class="form-select" id="donviphoihop" required>
