<style>
    #search {
        padding: 3px 12px;
        border-radius: 4px;
        outline: none;
        border: none;
        margin-right: 10px;
    }
</style>

<body>
    <div class="container-fluid">
        <div class="row">
            <%-include('./component/nav_chitietphong.ejs')%>
        </div>
        <div class="row">
            <div class="modal fade" id="modalRedirect" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
                <div class="modal-dialog">
                    <div class="modal-content" id="modal-content">
                        <div class="modal-header">
                            <h5 class="modal-title" id="exampleModalLabel">Quản trị chi tiết đơn vị cấp phòng, huyện, thị xã, thành phố</h5>
                            <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                        </div>
                        <div class="modal-body">
                            <div class="container-fluid">
                                <form id="cuocthiForm" class="row g-3">
                                    <div class="col-xl-12">
                                        <label for="ten" class="form-label">Chọn đơn vị muốn quản trị chi tiết tập thể đơn vị hoặc cá nhân: </label>
                                        <select id="donvi" class="form-select">
                                             <option value="" hidden disabled selected>-- Chọn mục này --</option>
                                             <% if(list.length > 0){
                                                  list.forEach(i=>{ %>
                                                       <option value="<%=i._id%>"><%=i.ten%></option>
                                               <%   })
                                             } %>
                                        </select>
                                    </div>
                                    <div class="col-12">
                                        <button id="redirect" class="btn btn-primary add" disabled type="submit">Đi tới trang</button>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!-- Modal add khen thưởng phòng, huyện, thị -->
            <div class="modal fade" id="modalAddKhenthuong" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
                <div class="modal-dialog">
                    <div class="modal-content" id="modal-content">
                        <div class="modal-header">
                            <h5 class="modal-title" id="exampleModalLabel">Thêm mới khen thưởng
                                <%=phong.ten%>
                            </h5>
                            <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                        </div>
                        <div class="modal-body">
                            <div class="container-fluid">
                                <form id="khenthuongForm" class="row g-3">
                                    <div class="col-md-3">
                                        <label for="soQD" class="form-label">Quyết định khen thưởng số: </label>
                                        <input type="text" name="soQD" class="form-control" id="soQD" required>
                                    </div>
                                    <div class="col-md-3">
                                        <label for="ngayQD" class="form-label">Ngày: </label>
                                        <input type="date" max="<%=maxDate%>" name="ngayQD" class="form-control" id="ngayQD" required>
                                    </div>
                                    <div class="col-md-3">
                                        <label for="hinhthuckhenthuong" class="form-label">Hình thức khen thưởng: </label>
                                        <select name="hinhthuckhenthuong" class="form-select" id="hinhthuckhenthuong" required>
                                            <option value="Giấy khen">Giấy khen</option>
                                            <option value="Bằng khen">Bằng khen</option>
                                            <option value="Cờ lưu niệm">Cờ lưu niệm</option>
                                        </select>
                                    </div>
                                    <div class="col-md-3">
                                        <label for="capkhenthuong" class="form-label">Cấp khen thưởng: </label>
                                        <select name="capkhenthuong" class="form-select" id="capkhenthuong" required>
                                            <option value="Công an tỉnh">Công an tỉnh</option>
                                            <option value="Bộ Công an">Bộ Công an</option>
                                            <option value="Trung ương">Trung ương</option>
                                            <option value="Tỉnh">Tỉnh</option>
                                            <option value="Huyện">Huyện</option>
                                        </select>
                                    </div>
                                    <div class="col-md-12">
                                        <label for="noidungkhenthuong" class="form-label">Nội dung khen thưởng: </label>
                                        <input type="text" name="noidungkhenthuong" class="form-control" id="noidungkhenthuong" required>
                                    </div>
                                    <div class="col-12">
                                        <button class="btn btn-primary add" type="submit"><i class="far fa-edit" style="margin-right: 3px;"></i>Thêm mới</button>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!-- End modal  -->
            <!-- Modal edit khen  phòng, huyện, thị  -->
            <div class="modal fade" id="modalEditKhenthuongPhong" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
                <div class="modal-dialog">
                    <div class="modal-content" id="modal-content">
                        <div class="modal-header">
                            <h5 class="modal-title" id="exampleModalLabel">Chỉnh sửa khen thưởng
                                <%=phong.ten%>
                            </h5>
                            <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                        </div>
                        <div class="modal-body">
                            <div class="container-fluid">
                                <form id="EditKhenthuongPhongForm" class="row g-3">
                                    <div class="col-md-3">
                                        <label for="soQD_Edit" class="form-label">Quyết định khen thưởng số: </label>
                                        <input type="text" name="soQD_Edit" class="form-control" id="soQD_Edit" required>
                                    </div>
                                    <div class="col-md-3">
                                        <label for="ngayQD_Edit" class="form-label">Ngày: </label>
                                        <input type="date" max="<%=maxDate%>" name="ngayQD_Edit" class="form-control" id="ngayQD_Edit" required>
                                    </div>
                                    <div class="col-md-3">
                                        <label for="hinhthuckhenthuong_Edit" class="form-label">Hình thức khen thưởng: </label>
                                        <select name="hinhthuckhenthuong_Edit" class="form-select" id="hinhthuckhenthuong_Edit" required>
                                            <option value="Giấy khen">Giấy khen</option>
                                            <option value="Bằng khen">Bằng khen</option>
                                            <option value="Cờ lưu niệm">Cờ lưu niệm</option>
                                        </select>
                                    </div>
                                    <div class="col-md-3">
                                        <label for="capkhenthuong_Edit" class="form-label">Cấp khen thưởng: </label>
                                        <select name="capkhenthuong_Edit" class="form-select" id="capkhenthuong_Edit" required>
                                            <option value="Công an tỉnh">Công an tỉnh</option>
                                            <option value="Bộ Công an">Bộ Công an</option>
                                            <option value="Trung ương">Trung ương</option>
                                            <option value="Tỉnh">Tỉnh</option>
