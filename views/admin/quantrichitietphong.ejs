                                        <label for="nam" class="form-label">Năm: </label>
                                        <input type="number" value='<%=year%>' name="nam" class="form-control" id="nam" required>
                                    </div>
                                    <div class="col-md-12">
                                        <label for="xeploai" class="form-label">Thành tích đạt được: </label>
                                        <input type="text" name="xeploai" class="form-control" id="xeploai" required>
                                    </div>
                                    <div class="col-md-12">
                                        <label for="ghichu" class="form-label">Ghi chú: </label>
                                        <input type="text" name="ghichu" class="form-control" id="ghichu">
                                    </div>
                                    <div class="col-12">
                                        <button class="btn btn-primary add" type="submit"><i class="far fa-edit" style="margin-right: 3px;"></i>Thêm mới</button>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!-- End vì an ninh  -->

            <!-- Edit thi đua năm  -->
            <div class="modal fade" id="modalEditThiduaPhong" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
                <div class="modal-dialog">
                    <div class="modal-content" id="modal-content">
                        <div class="modal-header">
                            <h5 class="modal-title" id="exampleModalLabel">Chỉnh sửa thi đua năm, phong trào Vì an ninh Tổ quốc
                                <%=phong.ten%>
                            </h5>
                            <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                        </div>
                        <div class="modal-body">
                            <div class="container-fluid">
                                <form id="editThiduaForm" class="row g-3">
                                    <div class="col-md-12">
                                        <label for="nam_Edit" class="form-label">Năm: </label>
                                        <input type="number" value='<%=year%>' name="nam_Edit" class="form-control" id="nam_Edit" required>
                                    </div>
                                    <div class="col-md-12">
                                        <label for="xeploai_Edit" class="form-label">Thành tích đạt được: </label>
                                        <input type="text" name="xeploai_Edit" class="form-control" id="xeploai_Edit" required>
                                    </div>
                                    <div class="col-md-12">
                                        <label for="ghichu_Edit" class="form-label">Ghi chú: </label>
                                        <input type="text" name="ghichu_Edit" class="form-control" id="ghichu_Edit">
                                    </div>
                                    <div class="col-12">
                                        <button class="btn btn-primary edit" type="submit"><i class="far fa-edit" style="margin-right: 3px;"></i>Cập nhật</button>
                                    </div>
