                        <div class="modal-body">
                            <div class="container-fluid">
                                <form id="addDoiForm" class="row g-3">
                                    <div class="col-md-12">
                                        <label for="ten" class="form-label">Tên đội nghiệp vụ, công an xã và tương đương: </label>
                                        <input type="text" name="ten" class="form-control" id="ten" required>
                                    </div>
                                    <div class="col-12">
                                        <button class="btn btn-primary add" type="submit"><i class="far fa-edit" style="margin-right: 3px;"></i>Thêm mới</button>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
