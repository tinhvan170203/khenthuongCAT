            <!-- Modal add khen thưởng cấp đội-->
            <div class="modal fade" id="modalAddKhenthuongDoi" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
                <div class="modal-dialog">
                    <div class="modal-content" id="modal-content">
                        <div class="modal-header">
                            <h5 class="modal-title" id="exampleModalLabel">Thêm mới khen thưởng đội nghiệp vụ, công an xã và tương đương</h5>
                            <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                        </div>
                        <div class="modal-body">
                            <div class="container-fluid">
                                <form id="khenthuongDoiForm" class="row g-3">
                                    <div class="col-md-4">
                                        <label for="soQD_Doi" class="form-label">Quyết định khen thưởng số: </label>
                                        <input type="text" name="soQD_Doi" class="form-control" id="soQD_Doi" required>
                                    </div>
                                    <div class="col-md-4">
                                        <label for="ngayQD_Doi" class="form-label">Ngày: </label>
                                        <input type="date" max="<%=maxDate%>" name="ngayQD_Doi" class="form-control" id="ngayQD_Doi" required>
                                    </div>
                                    <div class="col-md-4">
                                        <label for="hinhthuckhenthuong_Doi" class="form-label">Hình thức khen thưởng: </label>
                                        <select name="hinhthuckhenthuong_Doi" class="form-select" id="hinhthuckhenthuong_Doi" required>
                                            <option value="Giấy khen">Giấy khen</option>
                                            <option value="Bằng khen">Bằng khen</option>
                                            <option value="Cờ lưu niệm">Cờ lưu niệm</option>
                                        </select>
                                    </div>
                                    <div class="col-md-4">
                                        <label for="capkhenthuong_Doi" class="form-label">Cấp khen thưởng: </label>
                                        <select name="capkhenthuong_Doi" class="form-select" id="capkhenthuong_Doi" required>
                                            <option value="Công an tỉnh">Công an tỉnh</option>
