            <!-- Thêm cán bộ, chiến sĩ  -->
            <div class="modal fade" id="modalAddCanbo" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
                <div class="modal-dialog">
                    <div class="modal-content" id="modal-content">
                        <div class="modal-header">
                            <h5 class="modal-title" id="exampleModalLabel">Thêm mới cán bộ, chiến sĩ
                                <%=phong.ten%>
                            </h5>
                            <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                        </div>
                        <div class="modal-body">
                            <div class="container-fluid">
                                <form id="canboForm" class="row g-3">
                                    <div class="col-md-4">
                                        <label for="hoten" class="form-label">Họ và tên: </label>
                                        <input type="text" name="hoten" class="form-control" id="hoten" required>
                                    </div>
                                    <div class="col-md-4">
                                        <label for="ngaysinh" class="form-label">Ngày sinh: </label>
                                        <input type="date" max="<%=maxDate%>" name="ngaysinh" class="form-control" id="ngaysinh" required>
                                    </div>
                                    <div class="col-md-4">
                                        <label for="sohieuCAND" class="form-label">Số hiệu CAND: </label>
                                        <input type="text" id="sohieuCAND" class="form-control" name="sohieuCAND">
