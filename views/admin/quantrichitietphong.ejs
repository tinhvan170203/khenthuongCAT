            <!-- Thêm đội, công an xã  -->
            <div class="modal fade" id="modalAddDoi" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
                <div class="modal-dialog">
                    <div class="modal-content" id="modal-content">
                        <div class="modal-header">
